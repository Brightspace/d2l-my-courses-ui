{
  "name": "d2l-my-courses",
  "description": "Brightspace My Courses homepage widget",
  "private": true,
  "scripts": {
    "postinstall": "polymer install",
    "test": "npm run test:lint:js && npm run test:lint:wc && wct --skip-plugin sauce",
    "test:lint:js": "eslint --ext .js,.html .",
    "test:lint:wc": "polymer lint",
    "test:no-lint": "cross-env LAUNCHPAD_BROWSERS=chrome wct -p --skip-plugin sauce"
  },
  "homepage": "https://github.com/Brightspace/d2l-my-courses-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/d2l-my-courses-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Brightspace/d2l-my-courses-ui/issues"
  },
  "keywords": [
    "d2l",
    "homepage",
    "widget"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "cross-env": "^5.0.5",
    "eslint": "^4.11.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.0",
    "polymer-cli": "^1.5.7"
  }
}
